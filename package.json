{
  "name": "mute-core",
  "version": "0.3.1",
  "description": "",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "dependencies": {
    "mute-structs": "^0.3.2",
    "protobufjs": "^6.8.0",
    "rxjs": "^5.4.3",
    "safe-any": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coast-team/mute-core"
  },
  "devDependencies": {
    "@types/node": "^7.0.39",
    "ava": "^0.22.0",
    "tslint": "^3.15.1",
    "typescript": "^2.4.2"
  },
  "scripts": {
    "build": "tsc; cp -r proto/ dist/proto/",
    "clean": "rm -rf dist",
    "check": "tsc --noEmit",
    "lint": "tslint --project tsconfig.json",
    "test:unit": "npm run build && ava",
    "test": "npm run test:unit --",
    "proto": "pbjs -t static-module -w commonjs --no-verify --no-delimited --no-convert proto/collaborator.proto -o proto/collaborator_pb.js && pbts -o proto/collaborator_pb.d.ts proto/collaborator_pb.js && pbjs -t static-module -w commonjs --no-verify --no-delimited --no-convert proto/sync.proto -o proto/sync_pb.js && pbts -o proto/sync_pb.d.ts proto/sync_pb.js"
  },
  "ava": {
    "files": [
      "dist/test/**/*.test.js"
    ],
    "source": [
      "test"
    ],
    "timeout": "5s"
  },
  "keywords": [],
  "contributors": [
    "Philippe Kalitine <philippe.kalitine@gmail.com> (http://philippe.kalitine.name/)",
    "Matthieu Nicolas <matthieu.nicolas@inria.fr>"
  ],
  "license": "GPL-3.0"
}

{
  "name": "mute-core",
  "version": "0.1.7",
  "description": "",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "dependencies": {
    "google-protobuf": "^3.2.0",
    "mute-structs": "^0.1.3",
    "protobufjs": "^6.8.0",
    "rxjs": "^5.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coast-team/mute-core"
  },
  "devDependencies": {
    "@types/node": "^7.0.4",
    "ava": "^0.16.0",
    "tslint": "^3.15.1",
    "typescript": "2.3.4"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",
    "check": "tsc --noEmit",
    "lint": "tslint --project tsconfig.json",
    "test:unit": "npm run build && ava --verbose",
    "test": "npm run test:unit",
    "pbjs": "pbjs -t static-module -o proto/collaborator.js -w es6 --es6 proto/collaborator.proto && pbts -o proto/collaborator.d.ts proto/collaborator.js && pbjs -t static-module -o proto/sync.js -w es6 --es6 proto/sync.proto && pbts -o proto/sync.d.ts proto/sync.js"
  },
  "ava": {
    "require": ["babel-register"],
    "babel": "inherit"
  },
  "keywords": [],
  "contributors": [
    "Philippe Kalitine <philippe.kalitine@gmail.com> (http://philippe.kalitine.name/)",
